{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "API_VERSION": "v1",
    "API_TITLE": "Books API",
    "ENVIRONMENT": "production"
  },
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  }
}


